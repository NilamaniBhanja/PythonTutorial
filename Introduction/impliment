using HomeConnect.Domain.GCModels;

namespace HomeConnect.GCintegration;

public class XMLObjectConverter
{
    public ProposalDetails genProposalDetails()
    {
        ProposalDetails response = new ProposalDetails();
        
        response.generalProposalInformation = genGeneralProposalInformation();
        response.RiskDetails = genRiskDetails();

        return response;
    }

    private GeneralProposalInformation genGeneralProposalInformation()
    {
        GeneralProposalInformation generalProposalInformation = new GeneralProposalInformation
        {
            ErrorTrackingNeeded = true,
            ProductType = "MB",
            PropBranchDetails_IMDBranchCode = "BR123",
            PropBranchDetails_IMDBranchName = "Nilamani Branch",
            PropClauseDetails_DepartmentCode_Mandatary = 29.0
        };

        return generalProposalInformation;
    }

    private RiskDetails genRiskDetails()
    {
        RiskDetails riskDetails = new RiskDetails
        {
            PropCustomerDtls_CustomerName = "Nilamani"

        };

        return riskDetails;
    }


    //RiskDetails OtherDetailsGrid BPSooksmaFireSectionSI
    public BPSooksmaFireSectionSI genBPSooksmaFireSectionSI()
    {
        BPSooksmaFireSectionSI bPSooksmaFireSectionSI = new BPSooksmaFireSectionSI
        {
            ItemType = "Nilamani",
            SumInsured= 123234
        };
        return bPSooksmaFireSectionSI;
    }

    //RiskDetails OtherDetailsGrid SectionIAddonNonTerrorismCover
    public SectionIAddonNonTerrorismCover genSectionIAddonNonTerrorismCover()
    {
        SectionIAddonNonTerrorismCover sectionIAddonNonTerrorismCover = new SectionIAddonNonTerrorismCover
        {
            Description= "Nilamani",
            SumInsured = 123234
        };
        return sectionIAddonNonTerrorismCover;
    }

    //RiskDetails OtherDetailsGrid PropRiskCol
    public PropRiskCol genPropRiskCol()
    {
        PropRiskCol propRiskCol = new PropRiskCol
        {
           PropRisks_Section  = "Nilamani"
        };
        return propRiskCol;
    }


    //RiskDetails OtherDetailsGrid RiskGroup
    public RiskGroup genRiskGroup()
    {
        RiskGroup riskGroup = new RiskGroup
        {
            coverSIComponantDetails = new List<CoverSIComponantDetails>
            {
                new CoverSIComponantDetails()
                {
                    Name = "Section I Bharat Griha Raksha Material Damage",
                    Value = "10000",
                    Type = "GroupData",
                    CoverSIComponant = new List<CoverSIComponant>()
                    {
                        new CoverSIComponant()
                        {
                            PropCoverDetails_CoverGroups = "Section I Bharat Griha Raksha Material Damage",
                            PropCoverDetails_DifferentialSI =10000
                        }
                    }
                },
                 new CoverSIComponantDetails()
                {
                    Name = "Section I Terrorism Cover",
                    Value = "10000",
                    Type = "GroupData",
                    CoverSIComponant = new List<CoverSIComponant>()
                    {
                        new CoverSIComponant()
                        {
                            PropCoverDetails_CoverGroups = "Section I Bharat Griha Raksha Material Damage",
                            PropCoverDetails_DifferentialSI =10000
                        }
                    }
                }
            }
        };

        return riskGroup;
    }

}








    public XElement CreateRootXML()
    {
        //string productCode, string type, object objRisk, CommissionProposalInformation objGen
        HCXMLIntegration hCXMLIntegration = new HCXMLIntegration();
        XElement xmlRoot = hCXMLIntegration.CreateHomeComprehensivePackageXml();
        


        return xmlRoot;
    }
