private void SetYearRate(DomQuickQuote obj, string coverType, int year, decimal rate)
{
    var rateSetters = new Dictionary<string, Action<int, decimal>>
    {
        { "Consumable", (y, r) => SetConsumableYearRate(obj, y, r) },
        { "NilDep", (y, r) => SetNilDepYearRate(obj, y, r) },
        { "EngineCover", (y, r) => SetEngineCoverYearRate(obj, y, r) },
        { "GAP", (y, r) => SetGAPYearRate(obj, y, r) },
        { "KeyLoss", (y, r) => SetKeyLossYearRate(obj, y, r) },
        { "Towing", (y, r) => SetTowingYearRate(obj, y, r) },
        { "TyreProtection", (y, r) => SetTyreProtectionYearRate(obj, y, r) },
        { "DamageToCharger", (y, r) => SetDamageToChargerYearRate(obj, y, r) },
        { "ReinstallationOfWallMountedCharger", (y, r) => SetReinstallationYearRate(obj, y, r) },
        { "DamageToProperty", (y, r) => SetDamageToPropertyYearRate(obj, y, r) },
        { "ChargerLiabilityProtection", (y, r) => SetChargerLiabilityYearRate(obj, y, r) },
        { "PersonalBelonging", (y, r) => SetPersonalBelongingYearRate(obj, y, r) },
        { "RoadSideAssist", (y, r) => SetRoadSideAssistYearRate(obj, y, r) },
        { "Liberty", (y, r) => SetLibertyYearRate(obj, y, r) }
    };

    if (rateSetters.TryGetValue(coverType, out var setter))
    {
        setter(year, rate);
    }
}

private void SetConsumableYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.ConsumableRateYear2 = rate;
    else if (year == 3) obj.ConsumableRateYear3 = rate;
}

private void SetNilDepYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.NilDepreciationRateYear2 = rate;
    else if (year == 3) obj.NilDepreciationRateYear3 = rate;
}

private void SetEngineCoverYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.EngineSafeRateYear2 = rate;
    else if (year == 3) obj.EngineSafeRateYear3 = rate;
}

private void SetGAPYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.GapRateYear2 = rate;
    else if (year == 3) obj.GapRateYear3 = rate;
}

private void SetKeyLossYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.KeyLossRateYear2 = rate;
    else if (year == 3) obj.KeyLossRateYear3 = rate;
}

private void SetTowingYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.TowingRateYear2 = rate;
    else if (year == 3) obj.TowingRateYear3 = rate;
}

private void SetTyreProtectionYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.TyreProtectionRateYear2 = rate;
    else if (year == 3) obj.TyreProtectionRateYear3 = rate;
}

private void SetDamageToChargerYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.DamageToChargerRateYear2 = rate;
    else if (year == 3) obj.DamageToChargerRateYear3 = rate;
}

private void SetReinstallationYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.ReinstallationOfWallMountedChargerRateYear2 = rate;
    else if (year == 3) obj.ReinstallationOfWallMountedChargerRateYear3 = rate;
}

private void SetDamageToPropertyYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.DamageToPropertyRateYear2 = rate;
    else if (year == 3) obj.DamageToPropertyRateYear3 = rate;
}

private void SetChargerLiabilityYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.ChargerLiabilityProtectionRateYear2 = rate;
    else if (year == 3) obj.ChargerLiabilityProtectionRateYear3 = rate;
}

private void SetPersonalBelongingYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.PersonalBelongingRateYear2 = rate;
    else if (year == 3) obj.PersonalBelongingRateYear3 = rate;
}

private void SetRoadSideAssistYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2) obj.RoadSideAssistanceRateYear2 = rate;
    else if (year == 3) obj.RoadSideAssistanceRateYear3 = rate;
}

private void SetLibertyYearRate(DomQuickQuote obj, int year, decimal rate)
{
    if (year == 2)
    {
        if (obj.IsLibertyBatterySecureApplicable) obj.LibertyBatterySecureRateYear2 = rate;
        if (obj.IsLibertyAssureApplicable) obj.LibertyAssureRateYear2 = rate;
    }
    else if (year == 3)
    {
        if (obj.IsLibertyBatterySecureApplicable) obj.LibertyBatterySecureRateYear3 = rate;
        if (obj.IsLibertyAssureApplicable) obj.LibertyAssureRateYear3 = rate;
    }
}
