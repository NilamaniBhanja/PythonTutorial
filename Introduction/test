private void SetCommonRateProperties(addonCoverDtls item, DomQuickQuote obj, string coverType)
{
    decimal rate1 = Convert.ToDecimal(item.addonCoverRate1);
    
    var propertySetters = new Dictionary<string, Action<decimal>>
    {
        { "Consumable", (rate) => SetConsumableProperties(obj, rate) },
        { "NilDep", (rate) => SetNilDepProperties(obj, rate) },
        { "EngineCover", (rate) => SetEngineCoverProperties(obj, rate) },
        { "GAP", (rate) => SetGAPProperties(obj, rate) },
        { "KeyLoss", (rate) => SetKeyLossProperties(obj, rate) },
        { "Towing", (rate) => SetTowingProperties(obj, rate) },
        { "TyreProtection", (rate) => SetTyreProtectionProperties(obj, rate) },
        { "DamageToCharger", (rate) => SetDamageToChargerProperties(obj, rate) },
        { "ReinstallationOfWallMountedCharger", (rate) => SetReinstallationProperties(obj, rate) },
        { "DamageToProperty", (rate) => SetDamageToPropertyProperties(obj, rate) },
        { "ChargerLiabilityProtection", (rate) => SetChargerLiabilityProperties(obj, rate) },
        { "PersonalBelonging", (rate) => SetPersonalBelongingProperties(obj, rate) },
        { "RoadSideAssist", (rate) => SetRoadSideAssistProperties(obj, rate) }
    };

    if (propertySetters.TryGetValue(coverType, out var setter))
    {
        setter(rate1);
    }

    SetAdditionalYearRates(item, obj, coverType);
}

private void SetConsumableProperties(DomQuickQuote obj, decimal rate)
{
    obj.DefaultPerConsumable = rate;
    obj.PerConsumableEntered = rate;
    obj.ConsumableRateYear1 = rate;
    obj.IsConsumableApplicable = true;
}

private void SetNilDepProperties(DomQuickQuote obj, decimal rate)
{
    obj.DefaultPerNilDep = rate;
    obj.PerNilDepEntered = rate;
    obj.NilDepreciationRateYear1 = rate;
    obj.IsNilDepApplicable = true;
}

private void SetEngineCoverProperties(DomQuickQuote obj, decimal rate)
{
    obj.DefaultPerEngine = rate;
    obj.percentEngineCoverentered = rate;
    obj.EngineSafeRateYear1 = rate;
    obj.IsEngineCoverApplicable = true;
}

