using System;
using AVOGIModel.Models.Motor;
using BRMSintegration;
using BRMSIntegration.BRMSService;

namespace BRMSIntegration
{
    internal class AddonConfig
    {
        public void ConsumableConfig(addonCoverDtls item, DomQuickQuote obj)
        {
            obj.DefaultPerConsumable = Convert.ToDecimal(item.addonCoverRate1);
            obj.PerConsumableEntered = Convert.ToDecimal(item.addonCoverRate1);
            obj.IsConsumableApplicable = true;
            obj.ConsumableRateYear1 = Convert.ToDecimal(item.addonCoverRate1);
            if (item.statusDecline2 == "N")
            {
                obj.ConsumableRateYear2 = Convert.ToDecimal(item.addonCoverRate2);
            }
            if (item.statusDecline3 == "N")
            {
                obj.ConsumableRateYear3 = Convert.ToDecimal(item.addonCoverRate3);
            }

            if (obj.PolicyTenure == 1)
            {
                if (item.statusDecline1 == "Y")
                {
                    obj.IsConsumableApplicable = false;
                }
                else if (item.addonCoverRate1 == 0)
                {
                    obj.IsConsumableApplicable = false;
                }
            }
            else if (obj.PolicyTenure == 2)
            {
                if (item.statusDecline2 == "Y")
                {
                    obj.IsConsumableApplicable = false;
                }
                else if (item.addonCoverRate1 == 0 || item.addonCoverRate2 == 0)
                {
                    obj.IsConsumableApplicable = false;
                }
            }
            else if (obj.PolicyTenure == 3)
            {
                if (item.statusDecline3 == "Y")
                {
                    obj.IsConsumableApplicable = false;
                }
                else if (item.addonCoverRate1 == 0 || item.addonCoverRate2 == 0 || item.addonCoverRate3 == 0)
                {
                    obj.IsConsumableApplicable = false;
                }
            }
            else if (obj.PolicyTenure == 5)
            {
                if (item.statusDecline5 == "Y")
                {
                    obj.IsConsumableApplicable = false;
                }
                else if (item.addonCoverRate1 == 0 || item.addonCoverRate2 == 0 || item.addonCoverRate3 == 0 || item.addonCoverRate4 == 0 || item.addonCoverRate5 == 0)
                {
                    obj.IsConsumableApplicable = false;
                }
            }
        }
